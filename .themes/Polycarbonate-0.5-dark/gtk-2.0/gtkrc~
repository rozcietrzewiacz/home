style "button"			= "default"
{
  xthickness			= 2
  ythickness			= 2
  engine "svg"
  {
    
    image 
    {
      function			= BOX
      state				= NORMAL
      file				= "button-normal.svg"
      border			= { 8, 8, 8, 8 }
      stretch			= TRUE
    }	
    image
    {
      function			= BOX
      state				= PRELIGHT
      file				= "button-prelight.svg"
      border			= { 6, 7, 6, 7 }
      stretch			= TRUE
    }
    image
    {
      function			= BOX
      state				= ACTIVE
      file				= "button-pressed.svg"
      border			= { 8, 8, 7, 7 }
      stretch			= TRUE
    }	
    image 
   {
     function			= BOX
     state				= INSENSITIVE
     file				= "button-insensitive.svg"
     border			= { 6, 7, 6, 7 }
     stretch			= TRUE
    }
image
    {
      function			= FOCUS
      file              		= "focus_button.svg"
      border            		= { 6,4,6,4 }
      stretch           		= TRUE
    }

  }
}

